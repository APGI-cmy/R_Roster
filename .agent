---
id: r-roster-app
description: >
  R_Roster application repository for teacher absenteeism management.
  Full-stack application for managing teacher rosters and absences
  under Maturion governance framework.

repository:
  name: R_Roster
  owner: APGI-cmy
  type: application
  technology: web-application

governance:
  canon:
    repository: APGI-cmy/maturion-foreman-governance
    path: /governance/canon
    reference: main
  
  fpc_compliance:
    status: complete
    version: 1.0.0
  
  bindings:
    # Supreme authority
    - id: build-philosophy
      path: BUILD_PHILOSOPHY.md
      role: supreme-authority
    
    # Constitutional governance
    - id: agent-constitution
      path: governance/canon/AGENT_CONSTITUTION.md
      role: agent-doctrine
    - id: agent-contract-management
      path: governance/canon/AGENT_CONTRACT_MANAGEMENT_PROTOCOL.md
      role: constitutional-contract-protocol
      tier: 0
      enforcement: mandatory
    
    # FM authority and execution
    - id: fm-execution-mandate
      path: governance/canon/FM_EXECUTION_MANDATE.md
      role: fm-authority
    - id: fm-operational-guidance
      path: governance/canon/FM_OPERATIONAL_GUIDANCE.md
      role: fm-operations
    - id: fm-preauth-checklist
      path: governance/canon/FM_PREAUTH_CHECKLIST_CANON.md
      role: builder-gate
    
    # Builder appointment and lifecycle
    - id: builder-appointment
      path: governance/canon/BUILDER_APPOINTMENT_PROCESS.md
      role: builder-lifecycle
    - id: agent-recruitment
      path: governance/canon/AGENT_RECRUITMENT.md
      role: agent-legitimacy
    
    # QA and testing governance
    - id: qa-catalog-gate
      path: governance/canon/QA_CATALOG_GATE.md
      role: qa-enforcement
    - id: zero-test-debt
      path: governance/canon/zero-test-debt-constitutional-rule.md
      role: qa-enforcement
    - id: test-removal-governance
      path: governance/canon/TEST_REMOVAL_GOVERNANCE_GATE_LOCAL.md
      role: test-governance
    - id: zero-warning-debt
      path: governance/canon/ZERO_WARNING_TEST_DEBT_IMMEDIATE_REMEDY_DOCTRINE.md
      role: warning-enforcement
    - id: agent-qa-boundaries
      path: governance/canon/AGENT_SCOPED_QA_BOUNDARIES.md
      role: constitutional-boundary
    
    # PR gates and enforcement
    - id: pr-gate-requirements
      path: governance/canon/PR_GATE_REQUIREMENTS_CANON.md
      role: gate-enforcement
    - id: pr-gate-precondition
      path: governance/canon/PR_GATE_PRECONDITION_RULE.md
      role: gate-precondition
    
    # Escalation and procedures
    - id: escalation-procedures
      path: governance/canon/ESCALATION_PROCEDURES.md
      role: escalation-policy
    
    # Learning and improvement
    - id: learning-promotion
      path: governance/canon/LEARNING_PROMOTION_RULE.md
      role: learning-capture
    - id: failure-promotion
      path: governance/canon/FAILURE_PROMOTION_RULE.md
      role: failure-learning
    - id: bootstrap-learnings
      path: governance/canon/BOOTSTRAP_EXECUTION_LEARNINGS.md
      role: execution-learnings
    
    # Repository initialization
    - id: repo-initialization
      path: governance/canon/REPOSITORY_INITIALIZATION_AND_GOVERNANCE_SEEDING_PROTOCOL.md
      role: initialization-protocol
    - id: fpc-layerdown
      path: governance/canon/FPC_REPOSITORY_LAYERDOWN_GUIDE.md
      role: layerdown-protocol
    - id: cross-repo-layerdown
      path: governance/canon/CROSS_REPOSITORY_LAYER_DOWN_PROTOCOL.md
      role: cross-repo-governance
    
    # Governance completeness
    - id: governance-completeness
      path: governance/canon/GOVERNANCE_COMPLETENESS_MODEL.md
      role: completeness-validation
    
    # Execution discipline
    - id: execution-bootstrap-protocol
      path: governance/canon/EXECUTION_BOOTSTRAP_PROTOCOL.md
      role: execution-discipline
    
    # FM merge gate management
    - id: fm-merge-gate-management
      path: governance/canon/T0-014_FM_MERGE_GATE_MANAGEMENT_CANON.md
      role: merge-gate-authority
    
    # Automated deprecation detection (BL-026/T0-015)
    - id: automated-deprecation-detection
      path: governance/policies/AUTOMATED_DEPRECATION_DETECTION_GATE.md
      role: tier-0-constitutional
      learning_id: BL-026
      tier: 0
      enforcement: mandatory
    
    # Watchdog Quality Integrity Channel (QIW)
    - id: watchdog-quality-integrity-channel
      path: governance/canon/WATCHDOG_QUALITY_INTEGRITY_CHANNEL.md
      role: quality-integrity-enforcement
      tier: 0
      version: 1.0.0
      enforcement: mandatory
      summary: QIW observes build, lint, test, deployment, runtime logs; blocks QA on anomalies
    
  runbooks:
    # Test execution protocol
    - id: agent-test-execution-protocol
      path: governance/runbooks/AGENT_TEST_EXECUTION_PROTOCOL.md
      role: test-execution
      principle: ci-is-confirmation-not-diagnostic
      enforcement: mandatory

agents:
  foreman:
    - id: R_Roster-app_FM
      path: .github/agents/R_Roster-app_FM.md
      version: 3.0.0
      role: orchestration-authority
  
  governance:
    - id: governance-liaison
      path: .github/agents/governance-liaison.md
      version: 5.0.0
      role: governance-enforcement
  
  oversight:
    - id: codex-advisor
      path: .github/agents/CodexAdvisor-agent.md
      version: 5.0.0
      role: cross-repository-coordination
  
  builders:
    - id: api-builder
      path: .github/agents/api-builder.md
      role: backend-development
    - id: ui-builder
      path: .github/agents/ui-builder.md
      role: frontend-development
    - id: qa-builder
      path: .github/agents/qa-builder.md
      role: quality-assurance
    - id: schema-builder
      path: .github/agents/schema-builder.md
      role: database-schema
    - id: integration-builder
      path: .github/agents/integration-builder.md
      role: integration-development

capabilities:
  execute_changes: true
  modify_tests: true
  modify_migrations: false
  mechanical_fixes: true
  read_only: false
  advisory_only: false

constraints:
  governance_interpretation: forbidden
  scope_expansion: forbidden
  zero_test_debt: required
  build_to_green_only: true
  architecture_immutable_during_build: true
  secrets_and_env_config: forbidden

enforcement:
  on_scope_violation: halt_and_escalate
  on_governance_resolution_failure: halt
  escalation_target: Foreman

scope:
  repository: APGI-cmy/R_Roster
  
  allowed_paths:
    # Application code
    - "*.sh"
    - "*.md"
    
    # Architecture and documentation
    - "architecture/**"
    - ".architecture/**"
    
    # QA and testing
    - "qa/**"
    - ".qa/**"
    
    # Governance (governed changes only)
    - "governance/**"
  
  restricted_paths:
    # Repository contract
    - ".agent"
    
    # Agent contracts (require governance authority)
    - ".github/agents/**"
  
  escalation_required_paths:
    # Constitutional governance changes
    - "BUILD_PHILOSOPHY.md"
    - "governance/canon/**"
    
    # Supreme governance files
    - ".agent"
    - ".github/agents/R_Roster-app_FM.md"
    - ".github/agents/governance-liaison.md"
    - ".github/workflows/**"

evidence:
  architecture:
    path: .architecture/
    status: present
  
  qa:
    path: .qa/
    status: present

cross_references:
  repos:
    - id: maturion-foreman-governance
      repository: APGI-cmy/maturion-foreman-governance
      role: primary-governance

lifecycle:
  phase: operational
  status: governed

authority:
  human: Johan Ras
  foreman: R_Roster-app_FM Agent (v3.0.0)
  governance: Governance Liaison Agent (v5.0.0)
  
  escalation_path:
    - Builder → FM → Johan (operational)
    - Direct to Johan (constitutional)

---
